@article{Tranter2015,
author = {Tranter, Andrew and Sofia, Sarah and Seeley, Jake and Kaicher, Michael and McClean, Jarrod and Babbush, Ryan and Coveney, Peter V. and Mintert, Florian and Wilhelm, Frank and Love, Peter J.},
title = {The BravyiâKitaev transformation: Properties and applications},
journal = {International Journal of Quantum Chemistry},
volume = {115},
number = {19},
pages = {1431-1441},
keywords = {quantum chemistry, quantum simulation, quantum computing},
doi = {10.1002/qua.24969},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/qua.24969},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/qua.24969},
abstract = {Quantum chemistry is an important area of application for quantum computation. In particular, quantum algorithms applied to the electronic structure problem promise exact, efficient methods for determination of the electronic energy of atoms and molecules. The BravyiâKitaev transformation is a method of mapping the occupation state of a fermionic system onto qubits. This transformation maps the Hamiltonian of n interacting fermions to an -local Hamiltonian of n qubits. This is an improvement in locality over the JordanâWigner transformation, which results in an O(n)-local qubit Hamiltonian. We present the BravyiâKitaev transformation in detail, introducing the sets of qubits which must be acted on to change occupancy and parity of states in the occupation number basis. We give recursive definitions of these sets and of the transformation and inverse transformation matrices, which relate the occupation number basis and the BravyiâKitaev basis. We then compare the use of the JordanâWigner and BravyiâKitaev Hamiltonians for the quantum simulation of methane using the STO-6G basis. Â© 2015 Wiley Periodicals, Inc.},,
year = {2015}
}
